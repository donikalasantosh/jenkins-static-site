pipeline {
    agent any

    stages {
        stage('Clone Repo') {
            steps {
                git branch: 'main', url: 'https://github.com/donikalasantosh/jenkins-static-site.git'

            }
        }
        stage('Deploy') {
            steps {
                sh 'scp -r * ubuntu@54.165.197.171:/var/www/html/'
            }
        }
    }
}
